{
  "name": "scip-ts",
  "version": "0.0.1",
  "description": "A pure wasm build of the SCIP constraint solver (scipopt.org) with a convenient typescript interface.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/index.node.js",
      "browser": "./dist/index.browser.js",
      "default": "./dist/index.browser.js"
    }
  },
  "files": [
    "dist/",
    "build/scip.js",
    "build/scip.wasm"
  ],
  "scripts": {
    "build": "npm run build:wasm && npm run build:ts",
    "build:wasm": "cd build && ./build.sh",
    "build:ts": "tsc && rollup -c",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "scip",
    "constraint",
    "optimise",
    "lp",
    "mip",
    "milp"
  ],
  "author": "Guy Paterson-Jones",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^20.0.0",
    "rollup": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
